{
    "name": "glfw",
    "version": "3.3.2",
    "configs": {
        "macos-arm64-Release": {
            "cmake_preset": "macos-arm64-Release",
            "platform": "macos",
            "arch": "arm64",
            "build_type": "Release",
            "c_compiler": "clang",
            "cxx_compiler": "clang++",
            "os": "darwin",
            "stdlib": "libc++",
            "cxx_std": "c++20",
            "cxx_flags": ""
        },
        "macos-arm64-Debug": {
            "cmake_preset": "macos-arm64-Debug",
            "platform": "macos",
            "arch": "arm64",
            "build_type": "Debug",
            "c_compiler": "clang",
            "cxx_compiler": "clang++",
            "os": "darwin",
            "stdlib": "libc++",
            "cxx_std": "c++20",
            "cxx_flags": ""
        }
    },
    "options": {
        "BUILD_SHARED_LIBS": false
    }
}