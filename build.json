{
    "name": "glfw",
    "version": "3.3.2",
    "configs": {
        "macos-arm64-Release": {
            "platform": {
                "os": "macos",
                "arch": "arm64",
                "build_type": "Release"
            },
            "compiler": {
                "c_compiler": "clang",
                "cxx_compiler": "clang++",
                "os": "darwin",
                "stdlib": "libc++",
                "cxx_std": "c++20",
                "cxx_flags": "",
                "build": {
                    "deployment_target": "12.0"
                }
            }
        },
        "macos-arm64-Debug": {
            "platform": {
                "os": "macos",
                "arch": "arm64",
                "build_type": "Debug"
            },
            "compiler": {
                "c_compiler": "clang",
                "cxx_compiler": "clang++",
                "os": "darwin",
                "stdlib": "libc++",
                "cxx_std": "c++20",
                "cxx_flags": "",
                "build": {
                    "deployment_target": "12.0"
                }
            }
        }
    },
    "options": {
        "BUILD_SHARED_LIBS": false
    }
}